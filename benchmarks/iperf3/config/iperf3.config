[port]
shorttip: -p
longtip: --port
type: int
help: The port to use for running iperf3. For multi-parallel tests, ports will be assigned starting with this specified value
required: 0
default: 5201

[time]
shorttip: -t
longtip: --time
type: int
help: Time in seconds to transmit for
required: 0
default: 120

[parallel]
shorttip: -P
longtip: --parallel
type: list
help: List of parallelisms to test (e.g. 1,2,4,8...)
required: 0
default: 1

[window]
shorttip: -w
longtip: --window
type: list
help: List of window sizes to test (e.g. 64K,83K...)
required: 0
default: 87380

[mss]
shorttip: -M
longtip: --set-mss
type: list
help: List of MSS sizes to test (e.g. 128, 256, 512...)
required: 0
default: 1460

[type]
shorttip: -T
longtip: --type
type: str
help: Specify 1G, 25G, 50G, or 100G test. Assumes a machine has the specified interfaces present
required: 0
default: 1G
