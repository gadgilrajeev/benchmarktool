[iterations]
shorttip: -i
longtip: --iterations
type: str
help: Number of iterations for coremark
required = 0
default = 1

[copies]
shorttip: -c
longtip: --copies
type: str
help: Number of copies for the spec run
required = 0
default = 1

[configpath]
shorttip: -f
longtip: --configpath
type: str
help: Location of configuration file
required = 0
default = /opt/cpu2006

[type]
shorttip: -t
longtip: --type
type: str
help: Spec int or Spec fp
required = 0
default = int

[num_cores]
shorttip: -n
longtip: --num_cores
type: int
help: Number of cores to run spec on
required = 0
default = 20

[tune_var]
shorttip: -l
longtip: --tune_var
type: str
help: Tuning type base or peak
required = 0
default = base
